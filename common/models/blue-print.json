{
  "name": "BluePrint",
  "plural": "BluePrints",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "fields": {
      "type": "array",
      "required": false
    },
    "isFirstEdit": {
      "type": "Boolean",
      "required": false,
      "default": true
    },
    "isArchived": {
      "type": "Boolean",
      "required": false,
      "default": false
    },
    "isStockSerialized": {
      "type": "Boolean",
      "required": false,
      "default": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "orientation": {
      "type": "string",
      "required": false
    },
    "size": {
      "type": "string",
      "required": false
    },
    "quality": {
      "type": "string",
      "required": false
    },
    "template": {
      "type": "array",
      "required": false,
      "mysql": {
        "columnName": "Template",
        "dataType": "LONGTEXT",
        "nullable": "N"
      }
    },
    "googleFonts": {
      "type": "string",
      "required": false
    },
    "printBackground": {
      "type": "Boolean",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "certifications": {
      "type": "hasMany",
      "model": "Certification",
      "foreignKey": "bluePrintId",
      "options": {
        "nestRemoting": true
      }
    },
    "certificates": {
      "type": "hasMany",
      "model": "Certificate",
      "foreignKey": "bluePrintId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
